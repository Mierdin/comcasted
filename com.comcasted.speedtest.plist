<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.comcasted.speedtest</string>
	<key>ProgramArguments</key>
	<array>
		<string>$PWD/comcasted.sh</string>
		<string>/Users/$USER/Desktop/comcasted.csv</string>
		<string>$SSID</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/comcasted.log</string>
	<key>StandardOutPath</key>
	<string>/tmp/comcasted.log</string>
	<key>StartInterval</key>
	<integer>300</integer>
</dict>
</plist>
